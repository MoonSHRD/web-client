{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "transform-class-properties",
    ["react-css-modules", {
      "generateScopedName": "_[path]_[name]__[local]",
    }],
    ["module-resolver", {
      "root": ["./src"],
    }],
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
  ],
}
